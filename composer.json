{
    "name": "zaratedev/laravel-discounts",
    "description": "Apply discounts to your Eloquent models in Laravel",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "'Jonathan Zarate'",
            "email": "zaratedev@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/database": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "orchestra/testbench": "~3.6.0|~3.7.0|~3.8.0|^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Zaratedev\\Discounts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zaratedev\\Discounts\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zaratedev\\Discounts\\DiscountsServiceProvider"
            ],
            "aliases": {
                "Discounts": "Zaratedev\\Discounts\\Facades\\Discounts"
            }
        }
    }
}
